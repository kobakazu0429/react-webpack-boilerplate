      "eslint --ext .ts,.tsx ./src --fix",
      "git add"
    ]
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.0",
