  },
  "devDependencies": {
    "@kobakazu0429/eslint-config-typescript-react": "1.0.8",
    "@types/node": "12.12.48",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/styled-components": "5.1.1",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.5.3",
    "eslint": "7.2.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.21.2",
    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin": "4.1.6",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "style-loader": "1.2.1",
    "ts-loader": "7.0.5",
    "typescript": "3.9.6",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-merge": "4.2.2"
  }
